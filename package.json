{
  "name": "toy-engine",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "private": true,
  "main": "dist/toy-engine.umd.js",
  "module": "dist/toy-engine.es5.js",
  "typings": "dist/types/toy-engine.d.ts",
  "files": [
    "dist"
  ],
  "author": "Matt (Sanemat) <o.gata.ken@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"{src,test,example}/**/*.ts\"",
    "build": "tsc --module commonjs && typedoc --out docs --target es6 --theme minimal --mode file src",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {},
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.6.8",
    "cross-env": "^6.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.15.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "is-whitespace-character": "^1.0.3",
    "jimp": "^0.8.1",
    "math-clamp": "^1.0.0",
    "meow": "^5.0.0"
  }
}
